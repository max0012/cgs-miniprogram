.search {
    width: 100%;
    display: flex;
    flex-direction: row;
    height: 88rpx;
    background: #fff;
    align-items: center;
}

.searchItem {
    display: flex;
    flex-direction: row;
    width: 100%;
    height: 65rpx;
    margin: 0 30rpx;
    border-radius: 30rpx;
    background: #F5F5F5;
}

.searchItem image {
    height: 32rpx;
    width: 32rpx;
    align-self: center;
    margin: 0 20rpx;
}

.searchItem input {
    flex: 1;
    font-size: 28rpx;
    margin-top: 13rpx;
}