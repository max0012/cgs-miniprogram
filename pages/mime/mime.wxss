.user {
    width: 750rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 100% 100%;
    -moz-background-size: 100% 100%;
    padding: 50rpx 20rpx;
    margin: 0 0 15rpx 0;
    color: #fff;
}

.user .avatar {
    width: 120rpx;
    height: 120rpx;
    margin-left: 45rpx;
    margin-right: 26rpx;
    display: inline-block;
    border-radius: 50%;
}

.user .user-info {
    flex: 7;
    margin-top: 15rpx;
    text-align: left;
}

.user .user-info .userinfo-nickname {
    font-size: 33rpx;
}

.user .user-info .level {
    height: 48rpx;
    line-height: 50rpx;
    font-size: 27rpx;
    margin-left: 23rpx;
    padding: 0 18rpx;
    vertical-align: top;
    color: #CDB080;
}

.user .user-info .job-number {
    font-size: 28rpx;
}
