.user {
    width: 100%;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 100% 100%;
    color: #fff;
}

.user .user-box {
    margin: 50rpx 40rpx;
}

.user .avatar {
    width: 120rpx;
    height: 120rpx;
    display: inline-block;
    border-radius: 50%;
}

.user .user-info {
    flex: 7;
    margin-left: 26rpx;
    margin-top: 15rpx;
    text-align: left;
}

.user .user-info .userinfo-nickname {
    font-size: 32rpx;
}

.user .user-info .level {
    height: 48rpx;
    line-height: 50rpx;
    font-size: 25rpx;
    margin-left: 23rpx;
    padding: 0 18rpx;
    vertical-align: top;
    color: #cdb080;
}

.user .user-info .job-number {
    font-size: 23rpx;
    margin-top: 6rpx;
}

.my-account, .my-services {
    width: 100%;
    background-color: #fff;
}

.my-account {
    margin-top: 15rpx;
}

.my-account .title {
    margin-left: 35rpx;
    margin-top: 40rpx;
    font-size: 28rpx;
}

.my-account .account-info {
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 100% 100%;
    border-radius: 8rpx;
    margin: 40rpx 35rpx 35rpx 35rpx;
    color: #fff;
    font-size: 25rpx;
    font-weight: 300;
}

.my-account .account-line {
    opacity: 0.2;
    margin: 0 30rpx;
    border-bottom: 1px solid #fff;
}

.my-account .item-line {
    opacity: 0.2;
    width: 1rpx;
    border-right: 1px solid rgb(179, 142, 142);
}

.my-account .withdrawal-info {
    padding: 60rpx 0;
}

.my-account .withdrawal-info .my-item .item-title {
    margin-top: 20rpx;
}

.my-account .transaction-info {
    padding: 45rpx 0;
}

.my-account .transaction-info .my-item .item-title {
    margin-top: 18rpx;
}

.my-account .account-info .my-item .item-num {
    font-size: 28rpx;
    font-weight: 500;
}

.my-account .account-info .my-item .item-title {
    font-size: 23rpx;
}

.i {
    padding: 7rpx;
    border: solid #fff;
    border-width: 0 1px 1px 0;
    display: inline-block;
    transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
    margin: 3rpx 5rpx;
    opacity: 0.5;
}

.my-services .title {
    margin-left: 35rpx;
    margin-top: 5rpx;
    font-size: 28rpx;
}

.my-services .services-info {
    flex-wrap: wrap;
    align-items: center;
    justify-content: space-between;
    margin: 40rpx 0 150rpx 0;
    font-size: 25rpx;
}

.my-services .services-info .my-item {
    width: 24%;
    text-align: center;
}

.my-services .services-info .item-icon {
    padding-bottom: 15rpx;
}

.my-services .services-info .item-icon image {
    width: 48rpx;
    height: 48rpx;
}

.my-services .services-info .item-title {
    padding-bottom: 60rpx;
}
